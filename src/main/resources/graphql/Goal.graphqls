type GoalProjection {
    id: Int!
    title: String
    progress: Float
    competencyCycle: CompetencyCycleSummary
}

type CompetencyCycleSummary {
    id: Int!
    competencyCycleName: String
}

type GoalPagination {
    data: [GoalProjection!]
    pagination: Pagination!
}

type GoalProgress {
    employeeId: Int
    goalName: String
    profileImage: String
    goalProgress: Float
}